<!-- TODO: Use a helper for test_runs/id/packages -->
<%= link_to "package view", "/test_runs/#{@test_run.id}/packages" %><br>

<% @test_run.test_results.each do |test_result| %>

	<% test = test_result.test %>
	<% if test_result.result == "pass" %>
		<font color="green">
	<% else %>
		<font color="red">
	<% end %>

	<%= "#{test.package}.#{test.class_name}.#{test.name}" %></font><br>
<% end %>